	file = {Full Text:/Users/rikutakei/Zotero/storage/2P56ZH5D/Bennett et al. - 2021 - Clinical Characterization and Prediction of Clinic.pdf:application/pdf;Snapshot:/Users/rikutakei/Zotero/storage/PEJTEVBU/2781923.html:text/html},
}

@article{gamazon_gene-based_2015,
	title = {A gene-based association method for mapping traits using reference transcriptome data},
	volume = {47},
	copyright = {2015 Nature Publishing Group, a division of Macmillan Publishers Limited. All Rights Reserved.},
	issn = {1546-1718},
	doi = {10.1038/ng.3367},
	abstract = {Hae Kyung Im and colleagues report a method for predicting gene expression perturbations from genotype data after training on reference transcriptome data sets. Association of predicted gene expression with disease traits identifies known and new candidate disease genes.},
