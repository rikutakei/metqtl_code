@article{surendran_trans-ancestry_2016,
	title = {Trans-ancestry meta-analyses identify rare and common variants associated with blood pressure and hypertension},
	volume = {48},
	copyright = {2016 Nature Publishing Group},
	issn = {1546-1718},
	doi = {10.1038/ng.3654},
	abstract = {High blood pressure is a major risk factor for cardiovascular disease and premature death. However, there is limited knowledge on specific causal genes and pathways. To better understand the genetics of blood pressure, we genotyped 242,296 rare, low-frequency and common genetic variants in up to 192,763 individuals and used ∼155,063 samples for independent replication. We identified 30 new blood pressure– or hypertension-associated genetic regions in the general population, including 3 rare missense variants in RBM47, COL21A1 and RRAS with larger effects ({\textgreater}1.5 mm Hg/allele) than common variants. Multiple rare nonsense and missense variant associations were found in A2ML1, and a low-frequency nonsense variant in ENPEP was identified. Our data extend the spectrum of allelic variation underlying blood pressure traits and hypertension, provide new insights into the pathophysiology of hypertension and indicate new targets for clinical intervention.},
