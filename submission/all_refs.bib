	month = mar,
	year = {2022},
	keywords = {gout, glycolysis, AP-1 activation, JNK, macrophage, MSU crystals, SLC2A1, transcriptional regulation},
	pages = {110489},
	file = {ScienceDirect Full Text PDF:/Users/rikutakei/Zotero/storage/23SREDTW/Cobo et al. - 2022 - Monosodium urate crystals regulate a unique JNK-de.pdf:application/pdf;ScienceDirect Snapshot:/Users/rikutakei/Zotero/storage/5V2JXCH5/S2211124722002224.html:text/html},
}

@article{kobayashi_hyaluronan_2020,
