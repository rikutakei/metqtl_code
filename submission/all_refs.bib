	month = nov,
	year = {2023},
	keywords = {Monocytes and macrophages, Vaccines},
	pages = {7385},
	file = {Full Text PDF:/Users/rikutakei/Zotero/storage/BR2AZHNJ/Ferreira et al. - 2023 - Fatty acid desaturation and lipoxygenase pathways .pdf:application/pdf},
