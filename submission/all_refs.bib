@article{straton_epigenomic_2024,
	title = {Epigenomic {Reprogramming} in {Gout}},
	volume = {2},
	copyright = {http://creativecommons.org/licenses/by/3.0/},
	issn = {2813-4583},
	doi = {10.3390/gucdd2040023},
	abstract = {Gout is a crystal-induced arthropathy in which monosodium urate (MSU) crystals precipitate within joints as a result of persistent hyperuricemia and elicit an inflammatory response. An intriguing aspect is the occurrence of gout in only 10–15\% of hyperuricemic individuals, suggesting the presence of additional risk factors. Although MSU crystal deposition is widely recognized as the cause of gout flares, the variability in initiating the inflammatory response to hyperuricemia and MSU deposition is not well understood. Several studies bring up-to-date information about the environmental and genetic influences on the progression towards clinical gout. Elevated urate concentrations and exposure to different external factors precipitate gout flares, highlighting the potential involvement of epigenetic mechanisms in gouty inflammation. A better understanding of the alteration of the epigenetic landscape in gout may provide new perspectives on the dysregulated inflammatory response. In this review, we focus on understanding the current view of the role of epigenomic reprogramming in gout and the mechanistic pathways of action.},
	language = {en},
	number = {4},
	urldate = {2024-12-20},
	journal = {Gout, Urate, and Crystal Deposition Disease},
