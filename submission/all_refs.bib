	pages = {355--361},
	file = {ScienceDirect Full Text PDF:/Users/rikutakei/Zotero/storage/CERZUDFD/Netea et al. - 2011 - Trained Immunity A Memory for Innate Host Defense.pdf:application/pdf;ScienceDirect Snapshot:/Users/rikutakei/Zotero/storage/WBCS53DM/S1931312811001284.html:text/html},
}

