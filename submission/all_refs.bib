@article{wakefield_bayes_2009,
	title = {Bayes factors for genome-wide association studies: comparison with {P}-values},
	volume = {33},
	copyright = {© 2008 Wiley‐Liss, Inc.},
	issn = {1098-2272},
	shorttitle = {Bayes factors for genome-wide association studies},
	doi = {https://doi.org/10.1002/gepi.20359},
	abstract = {The Bayes factor is a summary measure that provides an alternative to the P-value for the ranking of associations, or the flagging of associations as “significant”. We describe an approximate Bayes factor that is straightforward to use and is appropriate when sample sizes are large. We consider various choices of the prior on the effect size, including those that allow effect size to vary with the minor allele frequency (MAF) of the marker. An important contribution is the description of a specific prior that gives identical rankings between Bayes factors and P-values, providing a link between the two approaches, and allowing the implications of the use of P-values to be more easily understood. As a summary measure of noteworthiness P-values are difficult to calibrate since their interpretation depends on MAF and, crucially, on sample size. A consequence is that a consistent decision-making procedure using P-values requires a threshold for significance that reduces with sample size, contrary to common practice. Genet. Epidemiol. 2008. © 2008 Wiley-Liss, Inc.},
	language = {en},
	number = {1},
	urldate = {2021-01-22},
	journal = {Genetic Epidemiology},
	author = {Wakefield, Jon},
	year = {2009},
	keywords = {Bonferroni correction, P-values, prior distributions, strength of evidence},
	pages = {79--86},
	file = {Full Text PDF:/Users/rikutakei/Zotero/storage/X5247DX9/Wakefield - 2009 - Bayes factors for genome-wide association studies.pdf:application/pdf;Snapshot:/Users/rikutakei/Zotero/storage/HEVAQSPM/gepi.html:text/html},
}

