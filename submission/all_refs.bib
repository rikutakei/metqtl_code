	keywords = {gout, arthritis, gene polymorphism},
	pages = {571--578},
	file = {Full Text PDF:/Users/rikutakei/Zotero/storage/3GFGAGRL/Gosling et al. - 2018 - Mitochondrial genetic variation and gout in Māori .pdf:application/pdf;Snapshot:/Users/rikutakei/Zotero/storage/LB3FAB43/571.html:text/html},
