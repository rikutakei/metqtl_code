	month = feb,
	year = {2021},
	pages = {1098},
	file = {Full Text PDF:/Users/rikutakei/Zotero/storage/TBKXGNWM/Shi et al. - 2021 - Population-specific causal disease effect sizes in.pdf:application/pdf;Snapshot:/Users/rikutakei/Zotero/storage/TY49RNKJ/s41467-021-21286-1.html:text/html},
